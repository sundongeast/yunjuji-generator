{
    "name": "yujuji/yunjuji-generator",
    "description": "yunjuji generator",
    "type": "library",
    "keywords": ["yunjuji", "generator"],
    "homepage": "https://github.com/tiandaye/yunjuji-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "tiandaye",
            "email": "q408596288@vip.qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "infyomlabs/laravel-generator": "5.5.x-dev",
        "laravelcollective/html": "^5.5.0",
        "infyomlabs/adminlte-templates": "5.5.x-dev",
        "infyomlabs/swagger-generator": "dev-master",
        "jlapp/swaggervel": "dev-master",
        "doctrine/dbal": "~2.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": "5.5.*",
        "laravel/browser-kit-testing": "^2.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Yunjuji\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Models\\": "tests/models",
            "Tests\\Controllers\\": "tests/controllers"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yunjuji\\Generator\\YunjujiGeneratorServiceProvider",
                "Collective\\Html\\HtmlServiceProvider::class",
                "Laracasts\\Flash\\FlashServiceProvider::class",
                "Prettus\\Repository\\Providers\\RepositoryServiceProvider::class",
                "\\InfyOm\\Generator\\InfyOmGeneratorServiceProvider::class",
                "\\InfyOm\\AdminLTETemplates\\AdminLTETemplatesServiceProvider::class"
            ],
            "aliases": {
                "YunjujiGenerator": "Yunjuji\\Generator\\Facades\\YunjujiGenerator",
                "Form": "Collective\\Html\\FormFacade::class",
                "Html": "Collective\\Html\\HtmlFacade::class",
                "Flash": "Laracasts\\Flash\\Flash::class"
            }

        }
    }
}
